{
    "openapi": "3.0.0",
    "info": {
        "title": "API de Wewelcom - Restaurantes y Autenticación",
        "description": "API para la gestión de restaurantes y autenticación de usuarios en la plataforma Wewelcom.",
        "contact": {
            "email": "danielpalenzuelamora90@gmail.com"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000/api/v1",
            "description": "Servidor de Desarrollo Local"
        },
        {
            "url": "https://wewelcom-api-production.up.railway.app/api/v1",
            "description": "Servidor de Producción"
        }
    ],
    "paths": {
        "/registro": {
            "post": {
                "tags": [
                    "Autenticación"
                ],
                "summary": "Registro de un nuevo usuario",
                "description": "Registra un nuevo usuario en la plataforma",
                "operationId": "a9e813a2ba281de4e308f3b715c624bb",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/UserRegister"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Usuario registrado con éxito",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "mensaje": {
                                            "type": "string",
                                            "example": "Usuario registrado con éxito"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Error de validación",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "mensaje": {
                                            "type": "string",
                                            "example": "Faltan campos por rellenar o los campos no son válidos"
                                        },
                                        "errores": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/login": {
            "post": {
                "tags": [
                    "Autenticación"
                ],
                "summary": "Inicio de sesión de usuario",
                "description": "Autentica un usuario y devuelve un token",
                "operationId": "a67f672dd0794a3cefddb0051a2e2f44",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "usuario@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "Password123!"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Inicio de sesión exitoso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "token": {
                                            "type": "string",
                                            "example": "eyJhbGciOiJI..."
                                        },
                                        "mensaje": {
                                            "type": "string",
                                            "example": "Inicio de sesión exitoso"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Credenciales incorrectas",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "mensaje": {
                                            "type": "string",
                                            "example": "Las credenciales proporcionadas son incorrectas"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Error de validación",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "mensaje": {
                                            "type": "string",
                                            "example": "Faltan campos por rellenar o los campos no son válidos"
                                        },
                                        "errores": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/logout": {
            "post": {
                "tags": [
                    "Autenticación"
                ],
                "summary": "Cerrar sesión",
                "description": "Elimina los tokens de acceso para cerrar sesión",
                "operationId": "6fa87e6ae9c508a3f355b05d51c4b335",
                "responses": {
                    "200": {
                        "description": "Sesión cerrada con éxito",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Sesión cerrada con éxito"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/restaurantes": {
            "get": {
                "tags": [
                    "Restaurantes"
                ],
                "summary": "Obtiene todos los restaurantes",
                "operationId": "0d8a8e04bc46b5f8a156641ef802c8f4",
                "responses": {
                    "200": {
                        "description": "Listado de restaurantes",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "restaurantes": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/Restaurante"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "Restaurantes"
                ],
                "summary": "Crear un nuevo restaurante",
                "operationId": "b20fbf63286a1a852e72d2bc3d347900",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Restaurante"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Restaurante creado con éxito",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Restaurante creado con éxito"
                                        },
                                        "restaurante": {
                                            "$ref": "#/components/schemas/Restaurante"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Error de validación",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Error en la creación del restaurante"
                                        },
                                        "errors": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/restaurantes/{nombre}": {
            "get": {
                "tags": [
                    "Restaurantes"
                ],
                "summary": "Buscar restaurante por nombre",
                "operationId": "0421e1ff41742f0c5b8b06c1629d4655",
                "parameters": [
                    {
                        "name": "nombre",
                        "in": "path",
                        "description": "Nombre (parcial o completo) del restaurante a buscar",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "example": "Nuevo Restaurante"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Lista de restaurantes encontrados",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Restaurante"
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "No se encontró ningún restaurante",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "No se encontró el restaurante con el nombre Nuevo Restaurante"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/restaurantes/{id}": {
            "put": {
                "tags": [
                    "Restaurantes"
                ],
                "summary": "Actualizar restaurante",
                "operationId": "40da1ff3ea6ad2c66ccb9d371fe69db7",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID del restaurante a actualizar",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Restaurante"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Restaurante actualizado con éxito",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Restaurante actualizado con éxito"
                                        },
                                        "restaurante": {
                                            "$ref": "#/components/schemas/Restaurante"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Restaurante no encontrado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "No se encontró el restaurante con el id 1"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Error de validación",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Error en la actualización del restaurante"
                                        },
                                        "errors": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "Restaurantes"
                ],
                "summary": "Eliminar restaurante",
                "operationId": "05047e94e410d49fba72478c3ee811c3",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID del restaurante a eliminar",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Restaurante eliminado correctamente",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Restaurante con ID 1 eliminado correctamente"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Restaurante no encontrado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "No se encontró el restaurante con el id 1"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "UserRegister": {
                "required": [
                    "name",
                    "email",
                    "password"
                ],
                "properties": {
                    "name": {
                        "type": "string",
                        "example": "Daniel"
                    },
                    "email": {
                        "type": "string",
                        "format": "email",
                        "example": "daniel@example.com"
                    },
                    "password": {
                        "type": "string",
                        "format": "password",
                        "example": "secret123"
                    }
                },
                "type": "object"
            },
            "Restaurante": {
                "title": "Restaurante",
                "description": "Modelo de restaurante",
                "properties": {
                    "id_restaurante": {
                        "description": "Class Restaurante",
                        "type": "integer",
                        "format": "int64",
                        "example": 1
                    },
                    "nombre_restaurante": {
                        "type": "string",
                        "example": "La Parrilla Feliz"
                    },
                    "direccion_restaurante": {
                        "type": "string",
                        "example": "Calle Falsa 123, Ciudad"
                    },
                    "telefono_restaurante": {
                        "type": "string",
                        "example": "+34 123 456 789"
                    }
                },
                "type": "object"
            },
            "User": {
                "title": "User",
                "description": "Modelo de usuario",
                "properties": {
                    "id": {
                        "description": "Class User",
                        "type": "integer",
                        "format": "int64",
                        "example": 1
                    },
                    "name": {
                        "type": "string",
                        "example": "Juan Pérez"
                    },
                    "email": {
                        "type": "string",
                        "format": "email",
                        "example": "juan.perez@example.com"
                    },
                    "email_verified_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2023-01-01T12:00:00Z",
                        "nullable": true
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2023-01-01T12:00:00Z",
                        "nullable": true
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2023-01-01T12:00:00Z",
                        "nullable": true
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "description": "Token de autenticación. Introduce 'Bearer ' seguido de tu token.",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            },
            "apiKey": {
                "type": "apiKey",
                "description": "API Key para acceder a recursos protegidos.",
                "name": "X-API-KEY",
                "in": "header"
            }
        }
    },
    "tags": [
        {
            "name": "Autenticación",
            "description": "Operaciones de registro, login y logout de usuarios."
        },
        {
            "name": "Restaurantes",
            "description": "Gestión de información de restaurantes."
        }
    ]
}